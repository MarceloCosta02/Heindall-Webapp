@model IEnumerable<PortalHeindall.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioIDAgencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioCNPJ)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioNivel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioBancoDestino)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioIDAgencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioCNPJ)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioNivel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioBancoDestino)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
