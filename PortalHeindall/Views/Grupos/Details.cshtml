@model AppHeindall.Models.Grupo

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-center">@ViewData["Title"]</h1>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Grupo</h4>
                    <hr />
                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoName)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoName)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoDescription)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoDescription)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoType)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoType)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoMetodo)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoMetodo)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoURL)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoURL)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoUser)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoUser)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoPassword)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoPassword)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.GrupoPort)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.GrupoPort)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.PublicKey)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.PublicKey)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.PrivateKey)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.PrivateKey)
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="text-right">
    <a href="@Url.Action("Edit", new { id = Model?.Id })" class="btn btn-primary">Edit</a>
    <a href="@Url.Action("Index")" class="btn btn-secondary">Back to List</a>
</div>
