@model IEnumerable<AppHeindall.Models.IntegradorDoUsuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Integrador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioIdAgencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginIntegradorUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SenhaIntegradorUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PortaIntegradorUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PublicKeyIntegradorUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrivateKeyIntegradorUsuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Usuario.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Integrador.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UsuarioIdAgencia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LoginIntegradorUsuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SenhaIntegradorUsuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PortaIntegradorUsuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PublicKeyIntegradorUsuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PrivateKeyIntegradorUsuario)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
